{
    "sgemm":{
        "tests":{
            "actual": "t(A)@B",
            "linalg_matmul": "linalg.matmul(t(A),B)"
        },
        "optimized": "sgemm"
    },
    "cse_addition":{
        "tests":{
            "actual": "t(A)@B + t(A)@B"
        },
        "optimized": "2*(t(A)@B)"
    },
    "cse_matmul_paranthesis":{
        "tests":{
            "actual": "t(t(A)@B)@(t(A)@B)"
        },
        "optimized": "S=t(A)@B; t(S)@S"
    },
    "cse_matmul_no_paranthesis":{
        "tests":{
            "actual": "t(t(A)@B)@t(A)@B"
        },
        "optimized": "S=t(A)@B; t(S)@S"
    },
    "matchain_rtol":{
        "tests":{
            "actual": "t(H)@H@x",
            "linalg_multidot": "linalg.multi_dot([t(H), H, x])"
        },
        "optimized": "t(H)@(H@x)"
    },
    "matchain_ltor":{
        "tests": {
            "actual": "t(y)@t(H)@H",
            "linalg_multidot": "linalg.multi_dot([t(y), t(H), H])"
        },
        "optimized": "(t(y)@t(H))@H"
    },
    "matchain_mixed":{
        "tests": {
            "actual": "t(H)@y@t(x)@H",
            "linalg_multidot": "linalg.multi_dot([t(H), y, t(x), H])"
        },
        "optimized": "(t(H)@y)@(t(x)@H)"
    },
    "mp_trmm":{
        "tests": {
            "actual": "A@B",
            "linalg_matmul": "linalg.matmul(A,B)"
        },
        "optimized": "trmm"
    },
    "mp_syrk":{
        "tests": {
            "actual": "A@B",
            "linalg_matmul": "linalg.matmul(A,B)"
        },
        "optimized": "syrk"
    },
    "mp_tridiag":{
        "tests": {
            "actual": "A@B",
            "linalg_matmul": "linalg.matmul(A,B)"
        },
        "optimized": "csr(A)@B"
    },
    "am_distributivity1":{
        "tests": {
            "actual": "A@B+ A@C"
        },
        "optimized": "A@(B+C)"
    },
    "am_distributivity2":{
        "tests": {
            "actual": "(A - t(H)@H)@x"
        },
        "optimized": "A@x - t(H)@(H@x)"
    },
    "am_blocked":{
        "tests": {
            "actual": "A@B",
            "linalg_matmul": "linalg.matmul(A,B)"
        },
        "optimized": "blocked matrix multiply"
    },
    "cm_loops":{
        "tests": {
            "actual": "for i in range(3): A@B ..."
        },
        "optimized": "A@B; for i in range(3): ..."
    },
    "cm_partial_op_sum":{
        "tests": {
            "actual": "(A+B)[2,2]"
        },
        "optimized": "A[2,2] + B[2,2]"
    },
    "cm_partial_op_prod":{
        "tests": {
            "actual": "(A@B)[2,2]"
        },
        "optimized": "dot(A[2,:],B[:,2])"
    }
}