{
    "mm_sgemm":{
        "tests":{
            "operator": "transpose(A)@B",
            "linalg_matmul": "linalg.matmul(transpose(A),B)"
        },
        "ref_positive": "sgemm"
    },
    "mm_trmm":{
        "tests": {
            "operator": "L@B",
            "linalg_matmul": "linalg.matmul(L,B)"
        },
        "ref_positive": "trmm"
    },
    "mm_syrk":{
        "tests": {
            "operator": "A@transpose(A)",
            "linalg_matmul": "linalg.matmul(A,transpose(A))"
        },
        "ref_positive": "syrk"
    },
    "mm_tridiag":{
        "tests": {
            "operator": "T@B",
            "linalg_matmul": "linalg.matmul(T,B)",
            "linalg_tridiagonal_matmul": "linalg.tridiagonal_matmul(T,B)"
        },
        "ref_positive": "csr(T)@B"
    },
    "cse_addition":{
        "tests":{
            "operator": "transpose(A)@B + transpose(A)@B"
        },
        "ref_positive": "2*(transpose(A)@B)"
    },
    "cse_matmul_paranthesis":{
        "tests":{
            "operator": "transpose(transpose(A)@B)@(transpose(A)@B)"
        },
        "ref_positive": "S=transpose(A)@B; transpose(S)@S"
    },
    "cse_matmul_no_paranthesis":{
        "tests":{
            "operator": "transpose(transpose(A)@B)@transpose(A)@B"
        },
        "ref_positive": "S=transpose(A)@B; transpose(S)@S"
    },
    "cse_suboptimal":{
        "tests":{
            "operator": "transpose(A)@B@transpose(A)@B@y"
        },
        "ref_positive": "transpose(A)@(B@(transpose(A)@(B@y))"
    },
    "matchain_rtol":{
        "tests":{
            "operator": "transpose(H)@H@x"
        },
        "ref_positive": "transpose(H)@(H@x)"
    },
    "matchain_ltor":{
        "tests": {
            "operator": "transpose(y)@transpose(H)@H"
        },
        "ref_positive": "(transpose(y)@transpose(H))@H"
    },
    "matchain_mixed":{
        "tests": {
            "operator": "transpose(H)@y@transpose(x)@H"
        },
        "ref_positive": "(transpose(H)@y)@(transpose(x)@H)"
    },
    "am_distributivity1":{
        "tests": {
            "operator": "A@B+ A@C"
        },
        "ref_positive": "A@(B+C)"
    },
    "am_distributivity2":{
        "tests": {
            "operator": "(A - transpose(H)@H)@x"
        },
        "ref_positive": "A@x - transpose(H)@(H@x)"
    },
    "am_blocked":{
        "tests": {
            "operator": "A@B",
            "linalg_matmul": "linalg.matmul(A,B)"
        },
        "ref_positive": "blocked matrix multiply"
    },
    "cm_loops":{
        "tests": {
            "operator": "for i in range(3): A@B ..."
        },
        "ref_positive": "A@B; for i in range(3): ..."
    },
    "cm_partial_op_sum":{
        "tests": {
            "operator": "(A+B)[2,2]"
        },
        "ref_positive": "A[2,2] + B[2,2]"
    },
    "cm_partial_op_prod":{
        "tests": {
            "operator": "(A@B)[2,2]"
        },
        "ref_positive": "dot(A[2,:],B[:,2])"
    }
}