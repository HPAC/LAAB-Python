{
    "mm_sgemm":{
        "tests":{
            "operator": "t(A)@B",
            "linalg_matmul": "linalg.matmul(t(A),B)"
        },
        "ref_positive": "sgemm"
    },
    "mm_trmm":{
        "tests": {
            "operator": "L@B",
            "linalg_matmul": "linalg.matmul(L,B)"
        },
        "ref_positive": "trmm"
    },
    "mm_syrk":{
        "tests": {
            "operator": "A@t(A)",
            "linalg_matmul": "linalg.matmul(A,t(A))"
        },
        "ref_positive": "syrk"
    },
    "mm_tridiag":{
        "tests": {
            "operator": "T@B",
            "linalg_matmul": "linalg.matmul(T,B)"
        },
        "ref_positive": "csr(T)@B"
    },
    "cse_addition":{
        "tests":{
            "operator": "t(A)@B + t(A)@B"
        },
        "ref_positive": "2*(t(A)@B)"
    },
    "cse_matmul_paranthesis":{
        "tests":{
            "operator": "t(t(A)@B)@(t(A)@B)"
        },
        "ref_positive": "S=t(A)@B; t(S)@S"
    },
    "cse_matmul_no_paranthesis":{
        "tests":{
            "operator": "t(t(A)@B)@t(A)@B"
        },
        "ref_positive": "S=t(A)@B; t(S)@S"
    },
    "cse_suboptimal":{
        "tests":{
            "operator": "t(A)@B@t(A)@B@y"
        },
        "ref_positive": "t(A)@(B@(t(A)@(B@y))"
    },
    "matchain_rtol":{
        "tests":{
            "operator": "t(H)@H@x",
            "linalg_multidot": "linalg.multi_dot([t(H), H, x])"
        },
        "ref_positive": "t(H)@(H@x)"
    },
    "matchain_ltor":{
        "tests": {
            "operator": "t(y)@t(H)@H",
            "linalg_multidot": "linalg.multi_dot([t(y), t(H), H])"
        },
        "ref_positive": "(t(y)@t(H))@H"
    },
    "matchain_mixed":{
        "tests": {
            "operator": "t(H)@y@t(x)@H",
            "linalg_multidot": "linalg.multi_dot([t(H), y, t(x), H])"
        },
        "ref_positive": "(t(H)@y)@(t(x)@H)"
    },
    "am_distributivity1":{
        "tests": {
            "operator": "A@B+ A@C"
        },
        "ref_positive": "A@(B+C)"
    },
    "am_distributivity2":{
        "tests": {
            "operator": "(A - t(H)@H)@x"
        },
        "ref_positive": "A@x - t(H)@(H@x)"
    },
    "am_transpose":{
        "tests": {
            "operator": "t(B)@A@t(A)@B"
        },
        "ref_positive": "S = t(A)@B; t(S)@S"
    },
    "cm_loops":{
        "tests": {
            "operator": "for i in range(3): A@B ..."
        },
        "ref_positive": "A@B; for i in range(3): ..."
    },
    "cm_partial_op_sum":{
        "tests": {
            "operator": "(A+B)[2,2]"
        },
        "ref_positive": "A[2,2] + B[2,2]"
    },
    "cm_partial_op_prod":{
        "tests": {
            "operator": "(A@B)[2,2]"
        },
        "ref_positive": "dot(A[2,:],B[:,2])"
    }
}